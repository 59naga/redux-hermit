{
  "name": "redux-hermit",
  "version": "0.0.0",
  "description": "promise middleware with performing a server-side-rendering after the completion of the promises",
  "main": "lib",
  "files": ["lib"],
  "scripts": {
    "start": "abigail test PKG",
    "test": "mocha --compilers js:espower-babel/guess",
    "cover": "npm-run-all cover:*",
    "cover:test": "nyc --reporter=lcov --reporter=text npm run test",
    "cover:report": "codeclimate-test-reporter < coverage/lcov.info",
    "build": "npm-run-all build:*",
    "build:compile": "babel -V && babel src --out-dir lib"
  },
  "dependencies": {
    "bluebird": "^3.3.4"
  },
  "devDependencies": {
    "abigail": "^0.1.2",
    "axios": "^0.9.1",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "caravan": "^0.0.2",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "espower-babel": "^4.0.1",
    "express": "^4.13.4",
    "immutable": "^3.7.6",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.3",
    "nyc": "^6.1.1",
    "power-assert": "^1.3.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "redux": "^3.3.1"
  },
  "keywords": [
    "react",
    "redux",
    "promise",
    "server-side-rendering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/redux-hermit.git"
